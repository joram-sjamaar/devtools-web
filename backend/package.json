{
  "name": "seminar-week4-server",
  "version": "1.0.0",
  "description": "REST-API for the TODO-list application. Part of the course on Development Tools at Saxion University",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "watch": "nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "jsdoc": "jsdoc server test -r -d jsdoc",
    "apidoc": "apidoc -i server/ -o apidoc/",
    "test": "mocha",
    "test:nyan": "mocha --reporter Nyan",
    "test:html": "mocha --reporter mocha-simple-html-reporter --reporter-options output=reports/mocha_test_report.html",
    "coverage": "nyc --reporter=html mocha"
  },
  "author": "Evert Duipmans",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "lodash": "^4.17.11",
    "npx": "^10.2.2"
  },
  "devDependencies": {
    "apidoc": "^0.20.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "jasmine": "^3.5.0",
    "jsdoc": "^3.6.3",
    "mocha": "^5.0.4",
    "mocha-simple-html-reporter": "^1.1.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "supertest": "^3.0.0"
  },
  "apidoc": {
    "apidoc": {
      "title": "Custom apiDoc browser title",
      "url": "http://localhost:3000/api",
      "sampleUrl": "http://localhost:3000/api"
    }
  }
}
